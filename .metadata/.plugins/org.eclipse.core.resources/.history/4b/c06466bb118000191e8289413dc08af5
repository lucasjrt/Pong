package br.facom.ufu.poo;

public class Coordenada {
	public int x, y;
	
	private Coordenada(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	@Override
	public boolean equals(Object o) {
		Coordenada c = (Coordenada) o;
		if (c.x == this.x && c.y == this.y)
			return true;
		return false;
	}
	
	@Override
	public String toString() {
		return ("(" + x + ", " + y + ")");
	}
}
